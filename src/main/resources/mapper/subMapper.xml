<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ssm.mapper.subMapper" >
    <resultMap id="subResultMap" type="com.ssm.entity.sub">
        <result property="sub_id" column="sub_id"/>
        <result property="sub_name" column="sub_name"/>
    </resultMap>




    <insert id="insertSub">
        insert into sub (sub_id,sub_name) values (#{sub.sub_id},#{sub.sub_name});
    </insert>


</mapper>